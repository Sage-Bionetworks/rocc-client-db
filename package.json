{
  "name": "@sage-bionetworks/rocc-db-client",
  "version": "0.1.0",
  "description": "Client for managing a ROCC DB instance.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "config": "export $(grep -v '^#' .env | xargs)",
    "build:live": "npm run config && nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts",
    "test": "npm run build && node dist/src/index.js --help",
    "package": "pkg .",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [],
  "author": {
    "name": "Thomas Schaffter",
    "email": "thomas.schaffter@protonmail.com",
    "url": "https://github.com/tschaffter"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "commander": "^8.2.0",
    "glob": "^7.2.0",
    "lodash-es": "^4.17.21",
    "mongoose": "^6.0.9",
    "validator": "^13.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.10.3",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "nodemon": "^2.0.13",
    "pkg": "^5.3.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "bin": {
    "rocc-db-client": "./dist/src/index.js"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "targets": [
      "node16-linux-x64"
    ],
    "outputPath": "dist"
  }
}
